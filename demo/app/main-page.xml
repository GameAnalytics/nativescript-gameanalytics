<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" class="page"
  xmlns:ui="nativescript-yourplugin">
  <StackLayout class="p-20">
    <Label text="{{ message }}" class="t-20 text-center c-black" textWrap="true"/>
    <Button text="Tap Me!" tap="{{onTap}}" class="btn btn-primary btn-active" />
    <Button text="Tap Me 2!" tap="{{onTap2}}" class="btn btn-primary btn-active" />
  </StackLayout>
</Page>
